{
  "name": "hello-ppx-esy",
  "version": "0.1.0",
  "description": "Reason workflow with Esy",
  "license": "MIT",
  "esy": {
    "build": "refmterr dune build -p ppx_42",
    "NOTE": "Optional release Section. Customizes result of `esy release`",
    "release": {
      "releasedBinaries": [
        "ppx_42.exe"
      ],
      "deleteFromBinaryRelease": [
        "*esy_ocaml_*",
        "*refmterr-*",
        "ocaml-*"
      ]
    }
  },
  "dependencies": {
    "@opam/dune": "*",
    "@opam/ocaml-migrate-parsetree": "^1.0.11",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": "~4.6.1000"
  },
  "resolutions": {
    "@esy-ocaml/esy-installer": "0.0.1",
    "@opam/menhir": "20171013",
    "@opam/re": "1.8.0"
  },
  "devDependencies": {
    "@esy-ocaml/merlin": "*",
    "ocaml": "~4.6.1000"
  }
}
